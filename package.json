    "main": "tailwind.config.js",
    "scripts": {
        "watch-css": "npx tailwindcss -i ./src/input-style.css -o ./public/style.css --watch",
        "lint": "npx eslint src",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "npx prettier src --check",
