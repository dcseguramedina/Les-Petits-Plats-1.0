        "format": "npm run prettier:fix && npm run lint:fix"
    },
    "author": "",
    "license": "",
    "description": "",
    "devDependencies": {
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "prettier": "3.3.3",
        "tailwindcss": "^3.4.3"
    }
}
